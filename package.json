{
    "name": "daniel-baez-app",
    "homepage": "https://github.com/daniel-baez/daniel-baez.github.io",
    "version": "0.0.1",
    "private": true,
    "dependencies": {
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    },
    "scripts": {
        "start": "shadow-cljs watch app",
        "build": "shadow-cljs release app",
        "clean": "rimraf public/js/*",
        "predeploy": "npm run clean && npm run build",
        "deploy": "npm run predeploy && gh-pages --nojekyll -d public -b website -u 'Daniel Baez <daniel@baezdaniel.cl>'",
        "gh-pages-help": "gh-pages --help"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "gh-pages": "^6.1.1",
        "rimraf": "^3.0.2",
        "shadow-cljs": "^2.11.23"
    }
}
